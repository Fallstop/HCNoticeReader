{
	"name": "hcnoticereader",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && yarn build:patch-mgjml",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build:patch-cf": "sed --in-place 's/(.\\+ instanceof Request ? .\\+) ?? //' ./.svelte-kit/cloudflare/_worker.js",
		"build:patch-mgjml": "sed --in-place 's/require.resolve(\".\\+\"),\\?//' ./build/server/chunks/*",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@fontsource/fira-sans": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@originjs/vite-plugin-commonjs": "^1.0.3",
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-cloudflare": "^2.0.1",
		"@sveltejs/adapter-node": "^1.1.8",
		"@sveltejs/kit": "^1.5.3",
		"@types/cookie": "^0.5.1",
		"@types/html-minifier": "^4.0.2",
		"@types/mjml": "^4.7.0",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"cronitor": "^2.3.3",
		"dayjs": "^1.11.7",
		"eslint": "^8.33.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mjml": "^4.13.0",
		"node-cache": "^5.1.2",
		"node-mailjet": "^6.0.2",
		"sass": "^1.58.0",
		"string-strip-html": "^13.1.0",
		"svelte": "^3.55.1",
		"svelte-calendar": "^3.1.6",
		"svelte-check": "^3.0.3",
		"svelte-content-loader": "^1.1.3",
		"svelte-gestures": "^1.4.1",
		"svelte-simple-modal": "^1.5.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"uuid": "^9.0.0",
		"vite": "^4.1.1"
	},
	"type": "module",
	"dependencies": {
		"form-data": "^4.0.0",
		"mailgun.js": "^8.0.6"
	}
}
