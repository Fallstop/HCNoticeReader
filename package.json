		"svelte-calendar": "^3.1.6",
		"svelte-check": "^3.0.2",
		"svelte-content-loader": "^1.1.3",
		"svelte-gestures": "^1.4.1",
		"svelte-simple-modal": "^1.4.6",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"uuid": "^9.0.0",
		"vite": "^4.0.4"
	},
	"type": "module",
	"dependencies": {
		"form-data": "^4.0.0",
		"mailgun.js": "^8.0.6"
	}
